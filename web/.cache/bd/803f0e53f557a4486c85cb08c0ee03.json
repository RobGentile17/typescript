tends Model<K>, K> {\n  regions: { [key: string]: Element } = {}\n\n  constructor(public parent: Element, public model: T) {\n    this.bindModel()\n  }\n\n  abstract template(): string\n\n  regionsMap(): { [key: string]: string } {\n    return {}\n  }\n\n  eventsMap(): { [key: string]: () => void } {\n    return {}\n  }\n\n  bindModel(): void {\n    this.model.on(\"change\", () => {\n      this.render()\n    })\n  }\n  bindEvents(fragment: DocumentFragment): void {\n    const eventsMap = this.eventsMap()\n\n    for (let eventKey in eventsMap) {\n      // eventKey will be click:button, or drag:div etc..\n      const [eventName, selector] = eventKey.split(\":\") // this basically says you will get back an eventName which will be the 'click' and then the selector which will be 'button'\n\n      fragment.querySelectorAll(selector).forEach((element) => {\n        element.addEventListener(eventName, eventsMap[eventKey])\n      })\n    }\n  }\n\n  mapRegions(fragment: DocumentFragment): void {\n    const regionsMap = this.regionsMap()\n\n    for (let key in regionsMap) {\n      const selector = regionsMap[key]\n      const element = fragment.querySelector(selector)\n\n      if (element) {\n        this.regions[key] = element\n      }\n    }\n  }\n\n  onRender(): void {}\n\n  render(): void {\n    this.parent.innerHTML = \"\" //this will empty the parent element\n\n    const templateElement = document.createElement(\"template\")\n    templateElement.innerHTML = this.template() // this will rerender the template\n\n    this.bindEvents(templateElement.content) //bind our events\n    this.mapRegions(templateElement.content)\n\n    this.onRender()\n\n    this.parent.append(templateElement.content) // add it to the page\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"0201b95b475257c644111ebd84bbbf7f","cacheData":{"env":{}}}